package de.monticore;

import de.monticore.expressions.*;
import de.monticore.statements.*;

grammar SCDemo extends SCBodies, /* needed due to a bug, must be first grammar*/
Statecharts, AssignmentExpressions, CommonExpressions, MCCommonStatements {
  start SCArtifact;


  // repeated due to a bug in overriding productions:
  UMLTBody implements SCTBody;      // use the UMLTbody for Transitions
  SCActionBody implements SCABody;  // use the SCActionBody for Actions
  SCHSBody implements SCSBody;      // use hierarchical state bodies

}